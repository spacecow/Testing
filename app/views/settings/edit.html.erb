<div class="form">
<%= error_messages_for :setting %>

<fieldset>
<legend><%= t('settings.editing') %></legend>

<% form_for @setting do |f| %>
	<%= render :partial => "form", :locals => { :f=>f, :label_text=>t('update') } %>
<% end %>

<div class="links">
<p>
  <%= link_to t('show'), @setting, :class=>"link" %> |
  <%= link_to t('view_all'), settings_path, :class=>"link" %>
</p>
</div>

</fieldset>
</div>