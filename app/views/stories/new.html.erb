<div class="form">
<fieldset class="form">
<legend class="form"><%= t( 'klasses.new' ) %></legend>

<div class="intro">
	<%= t('message.asterisk') %>
</div>

<% semantic_form_for @story do |f| %>
  <%= render "form", :f=>f, :label_text => t(:create) %>
<% end %>

<div id="links">
	<% if can? :index, Story %>
		<%= link_to t(:list,:object=>t('stories.title')), stories_path %>
	<% end %>
</div>

</fieldset>
</div>